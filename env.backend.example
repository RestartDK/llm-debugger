##############################
# BugPoint Backend (env.backend.example)
#
# Copy this file to `.env` at the project root and fill in the values.
##############################

# Public URL where the BugPoint frontend is hosted.
# Used in MCP tool responses to link users to the debugger UI.
# Example (deployed):
# BUGPOINT_UI_URL=https://your-frontend-host
BUGPOINT_UI_URL=

# Groq API key used for LLM-based analysis.
# Obtain this from your Groq account: https://console.groq.com/
GROQ_API_KEY=

# Groq API base URL. Usually `https://api.groq.com`.
GROQ_API_BASE_URL=https://api.groq.com


